<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.github.tanxiaofan.personal">

    <application
        android:name="com.github.tanxiaofan.base.BaseApp"
        android:allowBackup="false"
        android:icon="@mipmap/personal_login_ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/personal_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".settings.SettingsActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PersonalActivity" />
    </application>

</manifest>